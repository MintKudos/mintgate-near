{
    "name": "mintgate",
    "version": "0.1.0",
    "private": true,
    "main": "./lib/index.js",
    "scripts": {
        "build:contracts": "cargo build --target wasm32-unknown-unknown --release",
        "unit-test:contracts": "cargo test",
        "app-test:contracts": "yarn build:contracts && jest",
        "app-test:contracts:clean": "rm -rf neardev/",
        "ts:contracts": "yarn ts:contracts:nft && yarn ts:contracts:market",
        "ts:contracts:nft": "near-ts --no-now mg-core/src/lib.rs mg-nft/src/lib.rs > src/mg-nft.ts",
        "ts:contracts:market": "near-ts --no-now mg-core/src/lib.rs mg-market/src/lib.rs > src/mg-market.ts",
        "md:contracts": "near-md mg-nft/src/lib.rs > mg-nft/README.md && near-md mg-market/src/lib.rs > mg-market/README.md",
        "dev-deploy:contract": "rm -rf neardev/ && near dev-deploy --wasmFile target/wasm32-unknown-unknown/release/mg_nft.wasm",
        "deploy:contract": "near deploy --wasmFile target/wasm32-unknown-unknown/release/mg_nft.wasm",
        "build:lib": "rm -rf lib/ && yarn tsc",
        "lint:lib": "eslint src test"
    },
    "dependencies": {
        "big.js": "^6.0.3",
        "gh-pages": "^3.1.0",
        "nanoid": "^3.1.22",
        "near-api-js": "0.39.0"
    },
    "devDependencies": {
        "@jest/console": "^26.6.2",
        "@types/bn.js": "^5.1.0",
        "@types/bs58": "^4.0.1",
        "@types/jest": "^26.0.21",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "autobahn": "^20.9.2",
        "bn.js": "^5.0.0",
        "bs58": "^4.0.1",
        "chalk": "^4.1.0",
        "eslint": "^7.22.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "jest": "^27.0.0-next.6",
        "jest-environment-node": "^26.6.2",
        "js-sha256": "^0.9.0",
        "near-cli": "^1.5.4",
        "near-seed-phrase": "^0.1.1",
        "prettier": "^2.2.1",
        "ts-jest": "^27.0.0-next.10",
        "typescript": "^4.2.3"
    }
}
